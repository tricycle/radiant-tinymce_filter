<%-
include_javascript 'admin/sitemap'
include_javascript 'extensions/tiny_mce/tinymce_integration.js'
include_stylesheet 'extensions/tiny_mce/tinymce_integration.css'
-%>
<table id="wysiwyg-site-map" class="index">
  <tbody>
    <%- if @homepage -%>
    <%= render_node @homepage %>
    <%- else -%>
    <tr><td class="note">Your Site does not have any pages yet.</td></tr>
    <%- end -%>
  </tbody>
</table>
